package hw5;

public class Producer implements Runnable{

	private BoundedBuffer buf;
	
	public Producer(BoundedBuffer buf){
		this.buf = buf;
	}
	
	public static void main(String[] args) {
		
	}

	@Override
	public void run() {
		for (int i = 0 ; i < 10 ; i++){
			System.out.println(Thread.currentThread() + "> Value: " + i);
			buf.push(i);
		}
		notify();
	}
}
