package hw5;

public class Producer implements Runnable{

	public static void main(String[] args) {
		Thread t1 = new Thread(new Producer());
		Thread t2 = new Thread(new Producer());
		t1.start();
		t2.wait(t1);
		t2.start();
	}

	@Override
	public void run() {
		for (int i = 0 ; i < 10 ; i++){
			System.out.println(Thread.currentThread() + "> Value: " + i);
		}
	}
}
